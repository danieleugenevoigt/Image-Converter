{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "main-src-tauri-capabilities-default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    {
      "identifier": "core:path:allow-resolve-directory",
      "allow": [
        {
          "path": "/"
        }
      ]
    },
    {
      "identifier": "dialog:allow-open",
      "allow": []
    },
    {
      "identifier": "store:allow-get"
    },
    {
      "identifier": "store:allow-set"
    },
    {
      "identifier": "store:allow-save"
    },
    {
      "identifier": "store:allow-delete"
    },
    {
      "identifier": "store:allow-clear"
    },
    "store:default"
  ]
}